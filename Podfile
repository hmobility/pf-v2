# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'ParkingFriends' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Example
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'RxLocalizer'
  pod 'RxOptional'
  pod 'RxViewController'
  pod 'RxSwiftExt', '~> 5'
  pod 'RxBinding'
  pod 'RxBiBinding'
  pod 'RxKeyboard'
  pod 'RxCoreLocation', '~> 1.4'
  pod "RxGesture"
  
  pod 'SnapKit', '~> 5.0.0'
  
  pod 'Alamofire', '~> 4.7'
  pod 'AlamofireObjectMapper', '~> 5.2.1'
  pod 'ObjectMapper', '~> 3.4'
  
  # pods for Firebase
  pod 'Fabric', '~> 1.10.2'
  pod 'Crashlytics', '~> 3.14.0'
  pod 'Firebase/Analytics'
  
  pod 'SwiftyJSON', '~> 5.0.0'
  
  pod 'NMapsMap'                  # Naver Map
  
  pod 'SwiftMessages'             # Message View
  pod 'ActiveLabel'               # Underlined Tappable Label
  
  pod 'AnyFormatKit'              # String Formatter
  pod 'CryptoSwift'               # SHA256 password to string
  
  pod 'PullUpController'
  pod 'TagListView', '~> 1.3.2'
  
  pod 'RangeSeekSlider'
  
end

target 'ParkingFriendsTests' do
  inherit! :search_paths
  
  pod 'Firebase/Analytics'
      
end

target 'ParkingFriendsUITests' do
  inherit! :search_paths
  
  pod 'Firebase/Analytics'
      
